<div id="<%= dom_id location %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Address1:</strong>
    <%= location.address1 %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Address2:</strong>
    <%= location.address2 %>
  </div>
  <div>
    <strong class="block font-medium mb-1">City:</strong>
    <%= location.city %>
  </div>
  <div>
    <strong class="block font-medium mb-1">State:</strong>
    <%= location.state %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Postal code:</strong>
    <%= location.postal_code %>
  </div>
</div>
